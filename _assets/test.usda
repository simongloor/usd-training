#usda 1.0

def "World"
{
    def Xform "Box" (
        prepend references = @Box.usdc@
    )
    {
        float xformOp:rotateX = -90
        uniform token[] xformOpOrder = ["xformOp:rotateX"]
    }

    def Xform "Shelf" (
        prepend references = @Shelf.usdc@
    )
    {
        float xformOp:rotateX = -90
        uniform token[] xformOpOrder = ["xformOp:rotateX"]
    }
}

